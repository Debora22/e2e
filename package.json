  "scripts": {
    "build": "npm run build:browser && npm run build:gateway",
    "build:browser": "BUILD_TARGET=browser npm run build:projext",
    "build:browser:development": "BUILD_TARGET=browser npm run build:projext:development",
    "build:development": "npm run build:browser:development && npm run build:gateway:development",
    "build:gateway": "BUILD_TARGET=gateway npm run build:projext",
    "build:gateway:development": "BUILD_TARGET=gateway npm run build:projext:development",
    "build:projext": "./utils/scripts/build",
    "build:projext:development": "./utils/scripts/build-development",
    "e2e": "./utils/scripts/protractor",
    "e2e:live": "E2E_ENV=live npm run e2e",
    "e2e:local": "E2E_ENV=local npm run e2e",
    "e2e:staging": "E2E_ENV=staging npm run e2e",
    "postinstall": "npm run revision",
    "install-hooks": "./utils/hooks/install",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:full": "npm run lint:js:full && npm run lint:style:full",
    "lint:js": "./utils/scripts/lint-js",
    "lint:js:full": "./utils/scripts/lint-js-full",
    "lint:style": "./utils/scripts/lint-style",
